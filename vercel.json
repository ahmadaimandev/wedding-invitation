{
    "functions": {
        "api/**/*.php": {
            "runtime": "vercel-php@0.7.0"
        },
        "*.php": {
            "runtime": "vercel-php@0.7.0"
        },
        "admin/**/*.php": {
            "runtime": "vercel-php@0.7.0"
        },
        "auth/**/*.php": {
            "runtime": "vercel-php@0.7.0"
        }
    },
    "routes": [
        {
            "src": "/(assets/.*)",
            "dest": "/$1"
        },
        {
            "src": "/admin/(.*)",
            "dest": "/admin/$1"
        },
        {
            "src": "/auth/(.*)",
            "dest": "/auth/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}